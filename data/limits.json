{
	"supply_voltage" : {
		"topic" : "services.xbee.readings",
		"variables" : {
			"${SUPPVOLTS}" : { 
				"path" : "supply_voltage.value",
			    "format" : "{0:03.2f}" 
			},
			"${ADDR}" : { 
				"path" : "address" 
			},
			"${TIME}" : {
				"path" : "time_stamp",
				"selector" : "[012][0-9]:[0-5][0-9]:[0-5][0-9]"
			}
		},	
		"limits" : {
			"Warning" : {
				"op" : "in",
				"limit" : [2.35, 2.5],
				"variable" : "${SUPPVOLTS}"
			},
			"Critical" : {
				"op" : "lt",
				"limit" : [2.35],
				"variable" : "${SUPPVOLTS}"
			}			
		}
	}
}